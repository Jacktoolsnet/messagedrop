{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/assets/fonts/*.ttf",
          "/assets/icons/*.png",
          "/assets/markers/*.svg",
          "/assets/weather-icons/css/*.css",
          "/assets/weather-icons/fonts/*.ttf",
          "/assets/weather-icons/fonts/*.woff",
          "/assets/weather-icons/fonts/*.woff2"
        ]
      }
    }
  ]
}